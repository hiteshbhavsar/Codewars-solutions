public class Kata_findMissingLetter
{
 	public static char findMissingLetter(char[] array)
    {
    	char c=array[0];int iasc=-1;
    	for(int i=1;i<array.length;i++)
    	{
    		if(((int)(c)+i)!=(int)(array[i]))
    		{
    			iasc=((int)(c)+i);
    			break;
    		}
    	}
    	c=(char) iasc;
    	return c;
    
    }
}
